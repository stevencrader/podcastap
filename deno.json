{
  "lock": false,
  "tasks": {
    "start": "deno task --cwd=src start",
    "build": "deno task --cwd=src build",
    "preview": "deno task --cwd=src preview"
  },
  "exclude": [
    "**/_fresh/*",
    "**/node_modules/*"
  ],
  "imports": {
    "$deno_kv_oauth/": "https://raw.githubusercontent.com/denoland/deno_kv_oauth/49bfffb4e7915ac12f5d67b01b8f2f93bcfdede2/",
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "$fresh_seo/": "https://deno.land/x/fresh_seo@1.0.1/",
    "$xmlparser/": "https://deno.land/x/xmlparser@v0.2.0/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "@std/assert": "jsr:@std/assert@^0.222.1",
    "@std/dotenv": "jsr:@std/dotenv@^0.222.1",
    "@std/encoding": "jsr:@std/encoding@^0.222.1",
    "@std/http": "jsr:@std/http@^0.220.1",
    "http-link-header": "npm:http-link-header@1.1.3",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "tailwindcss": "npm:tailwindcss@3.4.3",
    "tailwindcss-animation-delay": "npm:tailwindcss-animation-delay@1.2.0",
    "tailwindcss-aria-attributes": "npm:tailwindcss-aria-attributes@2.0.1"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ]
  },
  "nodeModulesDir": true
}
